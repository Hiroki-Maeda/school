/* 406 */
#include <stdio.h>
int main(void){
  float a[5][5], b[5][5];
  a[0][0]= 2;
  a[1][0]= 4;
  a[2][0]= 1;
  a[3][0]= 3;
  b[0][0]= 6;
  b[1][0]= 1;
  b[2][0]= 4;
  b[3][0]= 5;
 
  printf("|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\n",a[0][0], a[1][0], a[2][0], a[3][0], a[0][0]+a[2][0], a[1][0]+a[4][0]);
  printf("|\t |\t+\t|\t |\t=\t|\t |\n");
  printf("|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\n",b[0][0], b[1][0], b[2][0], b[3][0], b[0][0]+b[2][0], b[1][0]+b[4][0]);
  printf("|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\n",a[0][0], a[1][0], a[2][0], a[3][0], a[0][0]*a[2][0], a[1][0]*a[4][0]);
  printf("|\t |\t*\t|\t |\t=\t|\t |\n");
  printf("|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\t\t|%1.0f\t%1.0f|\n",b[0][0], b[1][0], b[2][0], b[3][0], b[0][0]*b[2][0], b[1][0]*b[4][0]);

	 return 0;
	 }
